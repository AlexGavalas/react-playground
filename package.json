{
    "name": "playground",
    "version": "0.0.0",
    "scripts": {
        "clean": "rm -rf dist",
        "start": "vite",
        "main": "ts-node --project src/tsconfig.node.json --transpile-only src/main.ts",
        "server": "json-server --watch ./src/db.json --delay 1000",
        "release": "HUSKY_SKIP_HOOKS=1 standard-version",
        "lint": "eslint .",
        "test": "jest"
    },
    "private": true,
    "dependencies": {
        "@testing-library/jest-dom": "5.16.1",
        "@testing-library/react": "12.1.2",
        "@testing-library/user-event": "13.5.0",
        "@vitejs/plugin-react-refresh": "1.3.6",
        "axios": "0.24.0",
        "d3": "7.3.0",
        "jest": "27.4.7",
        "msw": "0.36.3",
        "react": "17.0.2",
        "react-dom": "17.0.2",
        "react-query": "3.34.7",
        "react-router-dom": "6.2.1",
        "rxjs": "7.5.1",
        "vite": "2.7.10"
    },
    "devDependencies": {
        "@reduxjs/toolkit": "1.7.1",
        "@types/d3": "7.1.0",
        "@types/jest": "27.4.0",
        "@types/node": "17.0.8",
        "@types/react": "17.0.38",
        "@types/react-dom": "17.0.11",
        "@typescript-eslint/eslint-plugin": "5.9.0",
        "@typescript-eslint/parser": "5.9.0",
        "@visx/axis": "2.4.0",
        "@visx/curve": "2.1.0",
        "@visx/event": "2.1.2",
        "@visx/gradient": "2.1.0",
        "@visx/group": "2.1.0",
        "@visx/marker": "2.4.0",
        "@visx/mock-data": "2.1.2",
        "@visx/react-spring": "2.4.0",
        "@visx/scale": "2.2.2",
        "@visx/shape": "2.4.0",
        "@visx/tooltip": "2.2.2",
        "commitizen": "4.2.4",
        "cz-conventional-changelog": "3.3.0",
        "eslint": "8.6.0",
        "eslint-plugin-react": "7.28.0",
        "formik": "2.2.9",
        "husky": "7.0.4",
        "json-server": "0.17.0",
        "react-redux": "7.2.6",
        "react-spring": "9.4.1",
        "standard-changelog": "2.0.27",
        "standard-version": "9.3.2",
        "ts-jest": "27.1.2",
        "ts-node": "10.4.0",
        "typescript": "4.5.4"
    },
    "husky": {
        "hooks": {
            "pre-commit": "yarn lint"
        }
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog"
        }
    }
}
